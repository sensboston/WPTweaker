      <value>1</value>
    </entry>
    <entry path="HKEY_LOCAL_MACHINE\Software\Microsoft\Settings\{69DAA7D1-09EA-4eae-A67E-56E4B0B4CA5B}\SecureItems" name="{1DEF9B7D-2322-40eb-A007-16A75D5CDA6F}" type="dword">
      <value>b0</value>
    </entry>
    <entry path="HKEY_LOCAL_MACHINE\Software\Microsoft\Settings\QuickSettingElements" name="{1DEF9B7D-2322-40eb-A007-16A75D5CDA6F}" type="dword">
      <value>7</value>
    </entry>
  </tweak>

  <tweak category="User interface" name="Boot screen logo" type="input" description="Change boot screen logo" helper="picture">
    <entry path="HKEY_LOCAL_MACHINE\SYSTEM\Shell\BootScreens" name="WPBootScreenOverride" type="string"/>
  </tweak>

  <tweak category="System" name="Save maps to SD card" type="toggle" >
    <entry path="HKLM\System\Maps\Storage\" name="UseExternalStorage" type="dword" default="0" >
      <value>1</value>
    </entry>
  </tweak>

  <tweak category="System" name="Use 64MB maps cache instead of 128MB" type="toggle" >
    <entry path="HKLM\System\Maps\Storage\" name="UseSmallerCache" type="dword" default="0" >
      <value>1</value>
    </entry>
  </tweak>

  <tweak category="System" name="Disable FM radio option" type="toggle" >
    <entry path="HKLM\Software\Microsoft\FMRadio\OEM" name="NotPresent" type="dword" default="0" >
      <value>1</value>
    </entry>
  </tweak>

  <tweak category="Network" name="Enable Data Sense WiFI hotspots" type="toggle" >
    <entry path="HKLM\Software\Microsoft\Data Sense" name="DSEnabled" type="dword" default="0" >
      <value>1</value>
    </entry>
  </tweak>

  <tweak category="User interface" name="Disable Start Menu Letters" type="toggle" reboot="true" >
    <entry path="HKLM\Software\Microsoft\Shell\Start" name="GroupingThreshold" type="dword" default="45" comparer=">" >
      <value>1000</value>
    </entry>
  </tweak>

  <tweak category="System" name="Allow disabling camera shutter sound" type="toggle" >
    <entry path="HKLM\Software\Microsoft\Photos\OEM" name="ShutterSoundUnlocked" type="dword" default="0" >
      <value>1</value>
    </entry>
  </tweak>

  <tweak category="Network" name="Enable WiFi static IP" type="toggle" >
    <entry path="HKLM\SYSTEM\ControlSet001\services\WiFiConnSvc\Parameters\Config" name="EnableStaticIP" type="dword" default="0" >
      <value>1</value>
    </entry>
  </tweak>

  <tweak category="Cell" name="Enable SIM Contacts in People" type="toggle" >
    <entry path="HKLM\Software\Microsoft\Contacts\Sim" name="EnableSIMAddressBookAndExport" type="dword" default="0" >
      <value>1</value>
    </entry>
  </tweak>

  <tweak category="System" name="Disable USB Control Panel" type="toggle" >
    <entry path="HKLM\Software\Microsoft\Shell\USB" name="DisableUSBControlPanel" type="dword" default="0" >
      <value>1</value>
    </entry>
  </tweak>

  <tweak category="System" name="Notify on weak charger" type="toggle" >
    <entry path="HKLM\SOFTWARE\Microsoft\Shell\USB" name="NotifyOnWeakCharger" type="dword" default="0" >
      <value>1</value>
    </entry>
  </tweak>

  <tweak category="User interface" name="Enable soft Back, Start and Search buttons" type="toggle" >
    <entry path="HKLM\SOFTWARE\Microsoft\Shell\NavigationBar" name="SoftwareModeEnabled" type="dword" default="0" >
      <value>1</value>
    </entry>
  </tweak>

  <tweak category="User interface" name="Enable swipe up to hide on soft navbar" type="toggle" >
    <entry path="HKLM\SOFTWARE\Microsoft\Shell\NavigationBar" name="IsSwipeUpToHideEnabled" type="dword" default="0" >
      <value>1</value>
    </entry>
  </tweak>

  <tweak category="User interface" name="Recent people in people hub" type="input" >
    <entry path="HKLM\Software\Microsoft\Pim\RecentList\8" name="MaxSize" type="dword" default="8" />
  </tweak>

  <tweak category="System" name="Screen timeout (on battery)" type="enum" reboot="true">
    <entry path="HKLM\SOFTWARE\Microsoft\Shell\Timeouts" name="DCUserIdle" type="dword">
      <value name="30 seconds">1e</value>
      <value name="1 minute">3c</value>
      <value name="3 minutes">b4</value>
      <value name="5 minutes">12c</value>
      <value name="15 minutes">384</value>
      <value name="30 minutes">708</value>
      <value name="Never">0</value>
    </entry>
  </tweak>

  <tweak category="System" name="Screen timeout (when charging)" type="enum" reboot="true">
    <entry path="HKLM\SOFTWARE\Microsoft\Shell\Timeouts" name="ACUserIdle" type="dword">
      <value name="30 seconds">1e</value>
      <value name="1 minute">3c</value>
      <value name="3 minutes">b4</value>
      <value name="5 minutes">12c</value>
      <value name="15 minutes">384</value>
      <value name="30 minutes">708</value>
      <value name="Never">0</value>
    </entry>
  </tweak>

  <tweak category="System" name="Limit volume on reboot" type="toggle" reboot="true" >
    <entry path="HKLM\Software\OEM\VolumeLimit" name="EnableVolumeLimit" type="dword" default="0">
      <value>1</value>
    </entry>
  </tweak>

  <tweak category="System" name="Volume limit on reboot" type="input" >
    <entry path="HKLM\Software\OEM\VolumeLimit" name="VolumeLimit" type="dword" min="0" max="1E" default="19">
      <value>1</value>
    </entry>
  </tweak>

  <tweak category="System" name="Start key wakes up the screen" type="toggle" reboot="true" >
    <entry path="HKLM\System\Keyboard" name="EnableStartOnIdle" type="dword" default="0">
      <value>1</value>
    </entry>
  </tweak>

  <tweak category="System" name="Data Activity indicators" type="toggle" reboot="true" >
    <entry path="HKLM\Software\Microsoft\Shell\OEM\SystemTray" name="DataActivity" type="dword" default="0">
      <value>1</value>
    </entry>
  </tweak>

  <tweak category="Network" name="WLAN instead of WiFi string" type="toggle" >
    <entry path="HKLM\Software\Microsoft\UIX\ProductFeatures" name="WiFiToWLAN" type="dword" default="0">
      <value>1</value>
    </entry>
  </tweak>

