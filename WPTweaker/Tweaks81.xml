  <tweak category="System" name="'Never' screen timeout option" type="toggle" description="Enable or disable 'Never' timeout" comparer="==">
    <entry path="HKLM\SOFTWARE\Microsoft\Settings\Lock" name="DisableNever" type="dword" default="1">
      <value>0</value>
    </entry>
  </tweak>

  <tweak category="User interface" name="Smaller text in system apps" type="enum" reboot="true">
    <entry path="HKLM\SOFTWARE\Microsoft\Windows\CurrentVersion\Control Panel\Theme" name="LargeScreen" type="dword" default="0">
      <value name="Small (normal text)">0</value>
      <value name="Medium (smaller text)">3f</value>
      <value name="Big (small text, 5 quick toggles)">4b</value>
    </entry>
  </tweak>

  <tweak category="User interface" name="Screen size" type="enum" reboot="true">
    <entry path="HKLM\SOFTWARE\Microsoft\Windows\CurrentVersion\Control Panel\Theme" name="UserPreferenceWidth" type="dword" default="0">
      <value name="Small (2 medium tiles)">0</value>
      <value name="Medium (3 medium tiles)">1</value>
      <value name="Big (3 medium tiles)">2</value>
    </entry>
  </tweak>

  <tweak category="User interface" name="Start screen size" type="enum" reboot="true">
    <entry path="HKLM\Software\Microsoft\Shell\OEM\\Start" name="ScreenSize" type="dword" default="0">
      <value name="Small (2 medium tiles)">0</value>
      <value name="Medium (3 medium tiles)">1</value>
      <value name="Big (3 medium tiles)">2</value>
    </entry>
  </tweak>

  <tweak category="User interface" name="Haptic feedback" type="toggle" description="Enable or disable haptic feedback" reboot="true">
    <entry path="HKEY_LOCAL_MACHINE\System\Touch\Buttons" name="Vibrate" type="dword" default="0">
      <value>1</value>
    </entry>
  </tweak>

  <tweak category="User interface" name="Touch buttons intensity" type="enum" description="Increase or decrease intensity of touch buttons">
    <entry path="HKLM\System\Touch\Buttons" name="Intensity" type="dword" default="36">
      <value name="Low">18</value>      
      <value name="Default">36</value>
      <value name="High">72</value>
    </entry>
  </tweak>

  <tweak category="Cell" name="Hide highest speed selection" type="toggle" >
    <entry path="HKLM\Software\Microsoft\Cellular\MVSettings\IMSISpecific\Default\CellUX" name="HideHighestSpeed" type="dword" default="0" >
      <value>1</value>
    </entry>
  </tweak>

  <tweak category="Cell" name="Hide mode selection" type="toggle" >
    <entry path="HKLM\Software\Microsoft\Cellular\MVSettings\IMSISpecific\Default\CellUX" name="HideModeSelection" type="dword" default="0" >
      <value>1</value>
    </entry>
  </tweak>

  <tweak category="Cell" name="Enable LTE" type="toggle" >
    <entry path="HKLM\Software\Microsoft\Cellular\MVSettings\IMSISpecific\Default\General" name="LTEEnabled" type="dword" default="0" >
      <value>1</value>
    </entry>
  </tweak>

  <tweak category="Cell" name="Force LTE" type="toggle" >
    <entry path="HKLM\Software\Microsoft\Cellular\MVSettings\IMSISpecific\Default\General" name="LTEForced" type="dword" default="0" >
      <value>1</value>
    </entry>
  </tweak>

