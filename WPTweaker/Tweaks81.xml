    <entry path="HKLM\SYSTEM\Platform\DeviceTargetingInfo" name="DevOSPreviewEnable" type="dword" default="0">
      <value>1</value>
    </entry>
  </tweak>

  <tweak category="Device Info" name="Phone Friendly Name" type="input" description="Change Phone Friendly Name">
    <entry path="HKLM\SYSTEM\Platform\DeviceTargetingInfo" name="PhoneFriendlyName" type="string"/>
  </tweak>

  <tweak category="Device Info" name="Phone Mobile Operator Display Name" type="input" description="Change Phone Operator Display Name">
    <entry path="HKLM\SYSTEM\Platform\DeviceTargetingInfo" name="PhoneMobileOperatorDisplayName" type="string"/>
  </tweak>

  <tweak category="Misc" name="IE easter egg" description="Apply tweak and type 'about:moz' in the IE address field (mostly useless)" type="toggle" >
    <entry path="HKLM\Software\Microsoft\Internet Explorer\Abouturls" name="moz" type="string" default="">
      <value>res://mshtml.dll/about.moz</value>
    </entry>
  </tweak>

  <tweak category="Cell" name="Unhide APN" type="toggle">
    <entry path="HKLM\Software\Microsoft\Cellular\MVSettings\IMSISpecific\Default\CellUX" name="HideAPN" type="dword" default="1">
      <value>0</value>
    </entry>
  </tweak>

  <tweak category="Cell" name="Unhide MMS APN" type="toggle">
    <entry path="HKLM\Software\Microsoft\Cellular\MVSettings\IMSISpecific\Default\CellUX" name="HideMMSAPN" type="dword" default="1">
      <value>0</value>
    </entry>
  </tweak>

  <tweak category="Cell" name="Hide SIM security UI" type="toggle">
    <entry path="HKLM\Software\Microsoft\Phone\Settings" name="HideSIMSecurityUI" type="dword" default="0">
      <value>1</value>
    </entry>
  </tweak>

  <tweak category="User interface" name="Enable Double Tap Off on soft navbar" type="toggle" >
    <entry path="HKLM\SOFTWARE\Microsoft\Shell\NavigationBar" name="IsDoubleTapOffEnabled" type="dword" default="0" >
      <value>1</value>
    </entry>
  </tweak>

  <tweak category="User interface" name="Color or Black on soft navbar" type="toggle" >
    <entry path="HKLM\SOFTWARE\Microsoft\Shell\NavigationBar" name="Color" type="dword" default="0" >
      <value>2</value>
    </entry>
  </tweak>

  <tweak category="User interface" name="Enable Auto Hide on soft navbar" type="toggle" >
    <entry path="HKLM\SOFTWARE\Microsoft\Shell\NavigationBar" name="IsAutoHideEnabled" type="dword" default="0" >
